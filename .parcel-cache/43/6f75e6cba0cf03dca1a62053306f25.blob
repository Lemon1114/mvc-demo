* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
*::after,
*::before {
  box-sizing: border-box;
}
ul,
ol {
  list-style: none;
}
section {
  display: inline-block;
  border: 1px solid #000;
  width: 50vw;
  height: 50vh;
}
body {
  overflow: hidden;
}
body > div {
  display: flex;
  flex-wrap: wrap;
}

.app1 {
  text-align: center;
}
.app1 button {
  padding: 5px 10px;
}
.app1 button:hover {
  cursor: pointer;
}

.app2 {
}
.app2 div {
  width: 100%;
}
.app2 .tab-bar {
  width: 100%;
  display: flex;
}
.app2 .tab-bar > li {
  width: 50%;
  border: 1px solid #000;
}
.app2 .tab-bar > li.active {
  border: none;
  background-color: rgb(247, 124, 9);
}
.app2 .tab-content > li {
  display: none;
}
.app2 .tab-content > li.active {
  display: block;
  background-color: rgb(247, 124, 9);
}

.app3 {
}
.app3 .slide {
  border: 1px solid #000;
  width: 50px;
  height: 50px;
}

.app4 {
  /* text-align: center; */
}
.app4 .gradient {
  display: inline-block;
  width: 200px;
  height: 200px;
  border: 1px solid #000;
  border-radius: 50%;
}

