.app4 .gradient {
  display: inline-block;
  width: 200px;
  height: 200px;
  border: 1px solid #000;
  border-radius: 50%;
}
@keyframes changeBgColor {
  0% {
    background-color: coral;
  }
  100% {
    background-color: crimson;
  }
}
.app4 .gradient.active {
  animation: changeBgColor 1s ease 0 infinite linear normal;
  animation: changeBgColor 1s linear 0 infinite;
}
